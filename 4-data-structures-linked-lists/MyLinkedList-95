// 10-->5-->16
// let myLinkedList = {
//   head: {
//     value: 10,
//     next: {
//       value: 5,
//       next: {
//         value: 16.
//         next: null
//       }
//     }
//   }
// }
using System;


public class MyLinkedList
{
  public MyLinkedList() // Constructor
  {
    LinkedList myLinkedList = new LinkedList(10); // Create with Value
    Console.WriteLine(myLinkedList);
  }
}

public class LinkedList
{
  Node _head;
  Node _tail;
  int _length;
  
  public LinkedList(int value) // Constructor
  {
    _head = new Node(value);
    _tail = _head;
    _length = 1;
  }
}

public class Node
{
  public int value { get; set; }
  public Node next { get; set; }

  public Node(int value) // Constructor
  {
    this.value = value;
    this.next = null;
  }
}